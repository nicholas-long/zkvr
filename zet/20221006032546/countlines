#!/usr/bin/awk -f
BEGIN { for (n = 1; n < ARGC; n++) if (ARGV[n] ~ /^[0-9]+$/) ARGV[n] = "zet/" ARGV[n] "/README.md" }
BEGINFILE { lines = 0 }
FNR == 1 { next }
/^# [Rr]elated/ { nextfile }
$0 !~ /^ *$/ { lines++ }
ENDFILE {
  id = FILENAME
  gsub(/^zet\//,"",id)
  gsub(/\/README.md$/,"",id)
  print id, FILENAME, lines
}
